@prefix : <http://www.geonames.org/ontology#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@base <http://www.geonames.org/ontology#> .

:alternateName a owl:DatatypeProperty ;
  rdfs:domain :Feature ;
  rdfs:subPropertyOf skos:altLabel 
.

:childrenFeatures a owl:ObjectProperty ;
  rdfs:comment "Links to an RDF document containing the descriptions of children features"@en ;
  rdfs:domain :Feature ;
  rdfs:label "children features"@en ;
  rdfs:range :RDFData
.

:Class a owl:Class ;
  rdfs:comment "A class of features."@en ;
  rdfs:label "Class"@en 
.

:Code a owl:Class ;
  rdfs:comment "A feature code."@en ;
  rdfs:label "Code"@en 
.

:colloquialName a owl:DatatypeProperty ;
  rdfs:label "colloquial name"@en ;
  rdfs:subPropertyOf :alternateName 
.

:countryCode a owl:DatatypeProperty ;
  rdfs:comment "A two letters country code in the ISO 3166 list"@en ;
  rdfs:comment "The countryCode value for a Geoname Feature is equal to the countryCode value of the parentCountry value."@en ;
  rdfs:domain :Feature ;
  rdfs:range xsd:string ;
  rdfs:label "ISO country code"@en 
.

:featureClass a owl:ObjectProperty ;
  rdfs:comment "The main category of the feature, as defined in geonames taxonomy."@en ;
  rdfs:domain :Feature ;
  rdfs:label "feature class"@en ;
  rdfs:range :Class 
.

:featureCode a owl:ObjectProperty ;
  rdfs:comment "Type of the feature, as defined in geonames taxonomy."@en ;
  rdfs:domain :Feature ;
  rdfs:label "feature code"@en ;
  rdfs:range :Code ;
  rdfs:subPropertyOf dct:type 
.

:Feature a owl:Class ;
  rdfs:comment "A geographical feature"@en ;
  rdfs:label "Feature"@en ;
  rdfs:subClassOf schema:Place ;
  rdfs:subClassOf geo:SpatialThing 
.

:GeonamesFeature a owl:Class ;
  rdfs:comment "A feature described in geonames database, uniquely defined by its geonames identifier"@en ;
  rdfs:label "Geonames Feature"@en ;
  rdfs:subClassOf :Feature ;
  skos:historyNote "Added in version 3.1"@en 
.

:geonamesID a owl:DatatypeProperty ;
  rdfs:label "geonames identifier"@en ;
  rdfs:subPropertyOf dct:identifier ;
  skos:historyNote "Added in version 3.1"@en 
.

:historicalName a owl:DatatypeProperty ;
  rdfs:label "historical name"@en ;
  rdfs:subPropertyOf :alternateName 
.

:locatedIn a owl:ObjectProperty ;
  rdfs:comment "Indicates that the subject resource is located in the object feature"@en ;
  rdfs:label "located in"@en ;
  rdfs:range :Feature 
.

:locationMap a owl:ObjectProperty ;
  rdfs:comment "A geonames map centered on the feature."@en ;
  rdfs:domain :Feature ;
  rdfs:label "map"@en ;
  rdfs:range :Map 
.

:Map a owl:Class .
:Map rdfs:comment "A Web page displaying a map"@en .
:Map rdfs:label "Map"@en .

:name a owl:DatatypeProperty ;
  rdfs:comment "The main international name of a feature. The value has no xml:lang tag."@en ;
  rdfs:domain :Feature ;
  rdfs:label "name"@en 
.

:nearbyFeatures a owl:ObjectProperty ;
  rdfs:comment "Links to an RDF document containing the descriptions of nearby features"@en ;
  rdfs:domain :Feature ;
  rdfs:label "nearby features"@en ;
  rdfs:range :RDFData 
.

:nearby a owl:ObjectProperty ;
  rdfs:comment "A feature close to the reference feature"@en ;
  rdfs:domain :Feature ;
  rdfs:label "nearby"@en ;
  rdfs:range :Feature 
.

:neighbour a owl:ObjectProperty ;
  a owl:SymmetricProperty ;
  rdfs:comment "A feature sharing a common boarder with the reference feature"@en ;
  rdfs:domain :Feature ;
  rdfs:label "neighbour"@en ;
  rdfs:range :Feature 
.

:neighbouringFeatures a owl:ObjectProperty ;
  rdfs:comment "Links to an RDF document containing the descriptions of neighbouring features. Applies when the feature has definite boarders."@en ;
  rdfs:domain :Feature ;
  rdfs:label "neighbouring features"@en ;
  rdfs:range :RDFData 
.

:officialName a owl:DatatypeProperty ;
  rdfs:comment "A name in an official local language"@en ;
  rdfs:domain :Feature ;
  rdfs:label "official name"@en ;
  rdfs:subPropertyOf skos:prefLabel
.

:parentADM1 a owl:ObjectProperty ;
  rdfs:domain :Feature ;
  rdfs:label "level 1 administrative parent"@en ;
  rdfs:subPropertyOf :parentFeature 
.

:parentADM2 a owl:ObjectProperty ;
  rdfs:domain :Feature ;
  rdfs:label "level 2 administrative parent"@en ;
  rdfs:subPropertyOf :parentFeature 
.

:parentADM3 a owl:ObjectProperty ;
  rdfs:domain :Feature ;
  rdfs:label "level 3 administrative parent"@en ;
  rdfs:subPropertyOf :parentFeature 
.

:parentADM4 a owl:ObjectProperty ;
  rdfs:domain :Feature ;
  rdfs:label "level 4 administrative parent"@en ;
  rdfs:subPropertyOf :parentFeature 
.

:parentCountry a owl:ObjectProperty ;
  rdfs:domain :Feature ;
  rdfs:label "parent country"@en ;
  rdfs:subPropertyOf :parentFeature 
.

:parentFeature a owl:ObjectProperty ;
  a owl:TransitiveProperty ;
  rdfs:comment "A feature parent of the current one, in either administrative or physical subdivision."@en ;
  rdfs:domain :Feature ;
  rdfs:label "parent feature"@en ;
  rdfs:range :Feature 
.

:population a owl:DatatypeProperty ;
  rdfs:domain :Feature ;
  rdfs:label "population"@en ;
  rdfs:range xsd:integer
.

:postalCode a owl:DatatypeProperty ;
  rdfs:domain :Feature ;
  rdfs:label "postal code"@en 
.

:RDFData a owl:Class ;
  rdfs:comment "A Document containing RDF description of one or several features."@en ;
  rdfs:label "RDF Data"@en 
.

:shortName a owl:DatatypeProperty ;
  rdfs:domain :Feature ;
  rdfs:label "short name"@en ;
  rdfs:subPropertyOf :alternateName 
.

:WikipediaArticle a owl:Class ;
  rdfs:label "Wikipedia Article"@en ;
  rdfs:comment "A Wikipedia article"@en 
.

:wikipediaArticle a owl:ObjectProperty ;
  rdfs:comment "A Wikipedia article of which subject is the resource."@en ;
  rdfs:domain :Feature ;
  rdfs:label "wikipedia article"@en ;
  rdfs:range :WikipediaArticle 
.
